



































































































































































/* 头部 */
.head.data-v-88f80252 {
	height: 100rpx;
	line-height:100rpx;
	border-top: #999999 1px solid;
	border-bottom: #999999 1px solid;
	overflow: hidden;
}
.itemst.data-v-88f80252 {
	float:left;
	overflow:hidden;
	text-align: center;
	width: 160rpx;
	padding-left: 20rpx;
	color: #808080;
}
/* 字体图标 */
/* 点击头部排序列表换字体颜色 */
.headerTxt.data-v-88f80252 {
	color: #fd964f;
}
.items-icon.data-v-88f80252 {
	padding-left:30rpx;
	float: left;
}
.icotop.data-v-88f80252 {
	width: 50rpx;
	top:0rpx;
	position: relative;
	height: 100rpx;
	float: left;
	height: 100rpx;
}
/* 排序字体图标 */
.iconfont.data-v-88f80252 {
	position: absolute;
	top: 0rpx;
	left: 0rpx;
	/* border: 1px solid; */
	height:100rpx;
	font-size: 45rpx;
	width: 50rpx;
	float: left;
	overflow: hidden;
	line-height: 100rpx;
	text-align: center;
}
/* 商品 列表*/
.dl.data-v-88f80252 {
	padding: 30rpx;
	border-bottom: #999999 1px solid;
	overflow: hidden;
}
.dl .dt.data-v-88f80252 {
	float: left;
	width: 250rpx;
	height: 250rpx;
}
.dl .dd.data-v-88f80252 {
	margin-left: 20rpx;
	width: 400rpx;
	float: left;
}
.dt image.data-v-88f80252 {
	width: 100%;
	height: 100%;
}
.dl .dd .span.data-v-88f80252,
.pinglun.data-v-88f80252 {
	color: #999999;
	font-size: 30rpx;
}
.dl .dd .name.data-v-88f80252 {
	padding-top: 10rpx;
	font-weight: 600;
}
/* 价格 */
.rmb.data-v-88f80252 {
	color: orange;
}
/* 筛选 */
.example-body.data-v-88f80252 {
	position: relative;
	/* padding: 30upx; */
	background: #fff;
}
.close.data-v-88f80252 {
	position: absolute;
	bottom: 0rpx;
	right: 0rpx;
	width: 100%;
	height: 100rpx;
}
/* 重置取消 */
.but1.data-v-88f80252,
.but2.data-v-88f80252 {
	font-size: 40rpx !important;
	font-weight: 600;
	border: none;
	border-radius: 0rpx;
	outline: none;
	float: left;
	display: block;
	height: 100rpx;
	width: 50%;
	line-height: 100rpx;
}
.but1.data-v-88f80252 {
	background-color: white;
}
.but2.data-v-88f80252 {
	background-color: orange;
}
.service.data-v-88f80252 {
	width: 100%;
	padding: 15rpx;
	overflow: hidden;
}
.service-text.data-v-88f80252 {
	padding: 15rpx;
	overflow: hidden;
}
.service-text ._span.data-v-88f80252 {
	padding:10rpx 0rpx;
	display: block;
	text-align: center;
	font-size: 36rpx;
	width: 160rpx;
	margin: 5%;
	float: left;
	background: #f8f9fb;
	border: 1rpx solid white;
}
/* 点击添加的样式 */
.active.data-v-88f80252 {
	border: 1rpx solid #fa8538 !important;
	background-color: #fce0d5;
	color: #fa9855;
}
/* 筛选结束 */

