#salesService {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #EEEEEE;
}
.line {
  width: 100%;
  height: 20rpx;
}
.tag {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
  background-color: white;
}
.apply,
.follow-up {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}
.action {
  border-bottom: 4rpx solid #FF6B01;
  color: #FF6B01;
}
.device,
.time {
  height: 70rpx;
  line-height: 70rpx;
  background-color: white;
  font-size: 30rpx;
}
.device text {
  margin-left: 30rpx;
  font-size: 30rpx;
}
.icon-gantan {
  color: #D1D1D1;
}
.Imei {
  width: 100%;
  padding: 30rpx 20rpx 40rpx;
  background-color: white;
}
.con-imei {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
  border-bottom: 1rpx solid #E0E0E0;
  background-color: white;
}
.Imei input {
  height: 80rpx;
  line-height: 80rpx;
}
.btn {
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #F0AD4E;
  color: #FF7200;
  text-align: center;
  margin-top: 10rpx;
  margin-right: 40rpx;
  font-size: 25rpx;
}
.time {
  padding-left: 30rpx;
  color: #D1D1D1;
}
.uni-list-cell-navigate image {
  width: 150rpx;
  height: 150rpx;
}
.uni-list-cell-navigate .btn {
  width: 140rpx;
  margin: 0px;
}
.uni-list-cell-navigate .text {
  width: 350rpx;
  font-size: 30rpx;
  color: #333333;
}
.inquire {
  position: fixed;
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.inquire text {
  margin: auto;
}

