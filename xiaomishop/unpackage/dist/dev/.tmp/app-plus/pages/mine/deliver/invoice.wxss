
page{
	  background-color: #F5F5F5;
}
.line {
	width: 100%;
	height: 20rpx;
	background-color: #F5F5F5;
}
.invoice-type,
.invoice-title {
	padding: 20rpx;
	font-size: 30rpx;
	background-color: white;
}
.invoice-title .btn {
	display: inline-block;
}
.title {
	margin-bottom: 40rpx;
}
.btn {
	width: 200rpx;
	height: 70rpx;
	line-height: 70rpx;
	background-color: #FCE0D5 !important;
	border: 2rpx solid #FA8538;
	color: #FA8538;
	text-align: center;
	margin-bottom: 20rpx;
	border-radius: 5rpx;
	font-size: 30rpx;
}
.units {
	width: 200rpx;
	height: 70rpx;
	line-height: 70rpx;
	background-color: #CACACA;
	margin-right: 30rpx;
	text-align: center;
	margin-bottom: 20rpx;
	border-radius: 5rpx;
	font-size: 30rpx;
	display: inline-block;
}
.txt {
	color: #CACACA;
	font-size: 28rpx;
}
.name,
.content,
.phone {
	height: 90rpx;
	line-height: 90rpx;
	padding-left: 20rpx;
	background-color: white;
	font-size: 30rpx;
}
.name {
	border-bottom: 2rpx solid #EBEDF0;
}
.information {
	padding-left: 20rpx;
	background-color: #F5F5F5;
	padding-top: 20rpx;
}
.information text {
	display: block;
	color: #999999;
	font-size: 30rpx;
	line-height: 50rpx;
}

